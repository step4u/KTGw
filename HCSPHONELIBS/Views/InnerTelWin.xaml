<Window x:Class="Com.Huen.Views.InnerTelWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="내선 추가" Height="144" Width="263" WindowStartupLocation="CenterOwner" ResizeMode="NoResize" Icon="/HCSPHONELIBS;component/icon.ico" ShowInTaskbar="False">
    <Grid>
        <TextBlock HorizontalAlignment="Left" TextWrapping="NoWrap" Text="내선 번호" VerticalAlignment="Top" Margin="10,10,0,0"/>
        <TextBox x:Name="txtTelnum" HorizontalAlignment="Left" Height="20" TextWrapping="NoWrap" VerticalAlignment="Top" Width="164" Margin="83,9,0,0" MaxLength="4" AutoWordSelection="True"/>
        <TextBlock HorizontalAlignment="Left" TextWrapping="NoWrap" Text="담 당 자" VerticalAlignment="Top" Margin="10,45,0,0"/>
        <TextBox x:Name="txtTellername" HorizontalAlignment="Left" Height="20" TextWrapping="NoWrap" VerticalAlignment="Top" Width="164" Margin="83,44,0,0" AutoWordSelection="True"/>
        <Button x:Name="btnAdd" Content="추가" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="83,85,0,0" Language="ko" Click="btnAdd_Click"/>
        <Button x:Name="btnCancel" Content="닫기" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="172,85,0,0" Click="btnCancel_Click"/>
    </Grid>
</Window>
